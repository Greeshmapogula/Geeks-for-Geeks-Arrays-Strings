#code
for _ in range(int(input())):
    s = input()
    s1 = input()
    c=0
    for i in range(len(s)):
        if s1[0]==s[i]:
                
            if s[i:i+len(s1)]==s1:
                print(i+1,end=" ")
                c=1
    if c==0:
        print(-1)
    else:
        print()
            
