#code
for _ in range(int(input())):
    n=int(input())
    l= list(map(int,input().split()))
    k=[]
    for i in range(0, len(l), n):
        k.append(l[i:i + n])
    #print(k)
    k.reverse()
    #print(k)
    r=[]
    for i in range(n):
        for j in range(n):
            print(k[j][i],end=' ')
    print()        
       
        
